<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.testemedia.mediacp2"
    android:versionCode="21"
    android:versionName="1.26" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="20" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name="com.testemedia.mediacp2.MainActivity"
            android:theme="@style/Theme.AppCompat.Light"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MenuTimetable"
            android:label="@string/timetable"
            android:screenOrientation="fullSensor" >
        </activity>

        <activity android:name=".Proeja"
            android:label="@string/proeja"
            android:screenOrientation="portrait" >
        </activity>

        <activity android:name=".PROEJA.Nota2cert"
            android:label="@string/proeja"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name=".PROEJA.ProejaAnual"
            android:label="@string/proeja"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name=".PROEJA.ProejaFinal"
            android:label="@string/proeja"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name=".PROEJA.ProejaPFV"
            android:label="@string/proeja"
            android:screenOrientation="portrait" >
        </activity>
        <activity android:name=".PROEJA.ProejaSobre"
            android:label="@string/proeja"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".CalcularMedias"
            android:label="@string/botaoMedias"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Info"
            android:label="@string/info"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Cadastrar"
            android:label="@string/cadastrar"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Boletim"
            android:label="@string/boletim"
            android:screenOrientation="sensorLandscape" >
        </activity>
        <activity
            android:name=".menucad"
            android:label="Menu Boletim"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Instrucoes"
            android:label="Como Utilizar"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".EditarLista"
            android:label="Editar/Remover Matérias"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".EditarMateria"
            android:label="Alterar Matéria"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Calendario"
            android:label="Calendario"
            android:screenOrientation="fullSensor" >
        </activity>
        <activity
            android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize" />
    </application>

</manifest>